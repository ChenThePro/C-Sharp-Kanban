<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBrush" Color="#5C6BC0"/>
    <SolidColorBrush x:Key="PrimaryLightBrush" Color="#C5CAE9"/>
    <SolidColorBrush x:Key="AccentBrush" Color="#FFF9C4"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="#F1F3F4"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#FBC02D"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="#212121"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="#757575"/>

</ResourceDictionary>